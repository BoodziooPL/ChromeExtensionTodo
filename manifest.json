{
  "manifest_version": 2,
  "name": "ToDoList",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "permissions": ["storage", "nativeMessaging"],
  "browser_action": {
    "default_popup": "popup/Popup.html",
    "default_title": "ToDoList"
  }
}
